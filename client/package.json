{
  "name": "q8m-client",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "q8m Client - Vue 3 frontend for Quiz 8 Mastery interview preparation platform",
  "author": "Hussein Tirawi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/htirawi/q8m.git"
  },
  "bugs": {
    "url": "https://github.com/htirawi/q8m/issues"
  },
  "homepage": "https://github.com/htirawi/q8m#readme",
  "keywords": [
    "q8m",
    "vue3",
    "typescript",
    "quiz",
    "interview",
    "preparation",
    "pinia",
    "vite",
    "pwa",
    "frontend"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:new": "./scripts/lint-new.sh",
    "lint:strict": "eslint src/ --ext .vue,.ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .vue,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "typecheck": "vue-tsc --noEmit",
    "type-check": "vue-tsc --noEmit",
    "prepare": "echo 'Husky installed at root level'",
    "validate": "pnpm lint:new && pnpm typecheck && pnpm test:unit",
    "validate:full": "pnpm lint && pnpm typecheck && pnpm test:unit",
    "pre-push": "pnpm typecheck && pnpm lint:new && pnpm test:unit && pnpm build"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.0",
    "@heroicons/vue": "^2.0.0",
    "@unhead/vue": "^1.9.0",
    "@vee-validate/zod": "^4.11.0",
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.0",
    "pinia": "^2.1.0",
    "vee-validate": "^4.11.0",
    "vue": "^3.4.0",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/vue": "^8.0.0",
    "@types/node": "^22.7.7",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.19.0",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "globals": "^16.4.0",
    "husky": "^8.0.3",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-pwa": "^0.17.5",
    "vitest": "^2.1.3",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
